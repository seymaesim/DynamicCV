@using EntityLayer.Concrete;
@model List<Skill>

<div class="col-md-6 col-sm-6">
    <h2 class="title"><strong>Some</strong> Skills</h2>
    <p>Generally all the technologies I work with</p>
    @foreach (var item in Model)
    {
        <h4 class="tm-progress-label">@item.Title <small class="progress-percent-small">@item.Value%</small></h4>
        <div class="progress tm-progress">
            <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: @item.Value%;"></div>
        </div>
    }

</div>

